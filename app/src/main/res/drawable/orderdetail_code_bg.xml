<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 填充颜色 -->
    <solid android:color="#fff8e6" />

    <!--
    dashGap：破折线之间的空隙
    dashWith：破折线的长度
    width：虚线的厚度
    -->
    <stroke
        android:dashGap="2dp"
        android:dashWidth="4dp"
        android:width="1dp"
        android:color="#f2d89f" />

</shape>